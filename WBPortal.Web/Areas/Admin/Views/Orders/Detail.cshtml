<script type="text/javascript" language="javascript">
    function DownloadCertificate(ExternalID) {
        window.open('/admin/orders/DownloadCertificate/' + ExternalID, 'Download', 'width=5,height=5')
    }
    function ResendApproval(ExternalID, ApproverEmail) {
        $.post("/admin/orders/ReSendApprovalEmail/" + ExternalID + '?@(SettingConstants.QS_EMAIL)=' + ApproverEmail,
                       function (data) {
                           alert(data);
                       });
    }
    function downloadauthfile()
    {        
        var flnm = "@(!string.IsNullOrEmpty(ViewBag.AuthFileName) ? Html.Raw(ViewBag.AuthFileName) : string.Empty)";
        var flcon = "@(!string.IsNullOrEmpty(ViewBag.AuthFileContent) ? Html.Raw(ViewBag.AuthFileContent) : string.Empty)";
        window.open('/admin/orders/Downloadauthfile?flnm=' + flnm+'&flcon='+flcon, 'Download', 'width=5,height=5')
    }
</script>
