                                                            <input name="button6" type="submit" class="bluebtnlite" id="button6" value="@WBSSLStore.Resources.GeneralMessage.Message.CancellationRefundRequest_btn" onclick="window.location.href='@Url.Action("cancellationrequest", "Support", new { area = "admin", id = Model.ID })    '" />
                                                        }
                                                    </td>
                                                    <td width="25%" align="center" valign="top">
                                                        @if (Model.OrderStatus == OrderStatus.ACTIVE && (Convert.ToString(ViewBag.CertificateStatus) != Convert.ToString(OrderStatus.PENDING)))
                                                        {
                                                            <input name="button2" type="button" class="bluebtnlite" id="button2" value="@WBSSLStore.Resources.GeneralMessage.Message.DownloadCertificate_btn" onclick="DownloadCertificate(@Model.ExternalOrderID)" />
                                                        }
                                                    </td>
                                                    <td width="42%" align="left" valign="top">
                                                        @if (Model.OrderStatus == OrderStatus.PENDING && !string.IsNullOrEmpty(@Model.ExternalOrderID) && !string.IsNullOrEmpty(Model.CertificateRequest.DomainName))
                                                        {
                                                            <input name="button3" type="button" class="bluebtnlite" id="button3" onclick="ResendApproval(@Model.ExternalOrderID,'@Model.CertificateRequest.CertificateApproverEmail    ')" value="@WBSSLStore.Resources.GeneralMessage.Message.ResendApprovalEmail_btn" />
                                                        }
                                                    </td>
                                                    @if (!string.IsNullOrEmpty(ViewBag.AuthFileName) && !string.IsNullOrEmpty(ViewBag.AuthFileContent))
                                                    {
                                                        <td width="42%" align="left" valign="top">
                                                            <input name="btnDownloadauthfile" type="button" class="bluebtnlite" id="btnDownloadauthfile" value="@WBSSLStore.Resources.GeneralMessage.Message.DownloadAuthFile_btnCaption" onclick="downloadauthfile()" />

                                                        </td>
                                                    }

                                                </tr>
                                            </table>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td align="left" valign="top">&nbsp;
                                        </td>
                                    </tr>
                                </table>
                            </td>
                        </tr>
                        <tr>
                            <td height="8" align="left" valign="top"></td>
                        </tr>
                    </table>
                </td>
            </tr>
        </table>
    </td>
</tr>
@{
    setting = null;
}
