<script type="text/javascript" language="javascript">

    var timez = "@WBSSLStore.Resources.ErrorMessage.Message.TimeZoneRequired"
    var Prefix = "@WBSSLStore.Resources.ErrorMessage.Message.InvoicePrifxRequired"
    var Vatcountry = "@WBSSLStore.Resources.ErrorMessage.Message.VatCountryRequired"
    var VatNo = "@WBSSLStore.Resources.ErrorMessage.Message.VatNoRequired"
    var AdminEmail = "@WBSSLStore.Resources.ErrorMessage.Message.AdminEmailRequired"
    var SuppEmail = "@WBSSLStore.Resources.ErrorMessage.Message.SupportEmailRequired"
    var BillEmail = "@WBSSLStore.Resources.ErrorMessage.Message.SupportEmailRequired"
    var BlogUrl = "@WBSSLStore.Resources.ErrorMessage.Message.BlogUrlRequired"
    var fb = "@WBSSLStore.Resources.ErrorMessage.Message.FbValidUrl"
    var twitter = "@WBSSLStore.Resources.ErrorMessage.Message.TwitterValidURL"
    var validvat = "@WBSSLStore.Resources.ErrorMessage.Message.ValidVatNo"
    var VatPer = "@WBSSLStore.Resources.ErrorMessage.Message.VatPerRequired"
    var page = "@WBSSLStore.Resources.ErrorMessage.Message.PageSizeRequired"
    var Commonnm = "@WBSSLStore.Resources.ErrorMessage.Message.CommonNameRequired"
    var currency = "@WBSSLStore.Resources.ErrorMessage.Message.Currency"
    var Brand = "@WBSSLStore.Resources.ErrorMessage.Message.BrandSettingUpdate"
    var deleSucss = "@WBSSLStore.Resources.GeneralMessage.Message.Delete_Sucessfully_Caption"
    var Unabletodel = "@WBSSLStore.Resources.GeneralMessage.Message.Unable_toDelete_Caption"

    function setDefaultlanguage() {

        var lang = "@ViewBag.Language";
        if (lang == '')
            $("#rbtEnglish").attr('checked', 'checked');
    }
    // starting the script on page load
    $(document).ready(function () {

        //jQuery.noConflict();
        isReloadOnClose = true;
       
        var flag = $("#txtCommonName").attr('title');
        if (flag != '' && flag == 'true') {
            $("#txtCommonName").attr('disabled', 'disabled');
        }

        ShowHideFun(document.getElementById('chkVAT').checked)
        $("#frmSetting").validate({
            rules: {
                ddCurrancy: "required",
                drpTimezone: "required",
                txtCommonName: {
                    required: true
                },
                drpVATCountry: {
                    required: "#chkVAT:checked"
                },
                txtVATNumber: {
                    required: "#chkVAT:checked",
                    number: true
                },
                fupLogo: {
                    accept: "gif|jpeg|png|jpg|bmp"
                },
               
                txtAdminEmail: {
                    required: true,
                    email: true
                },
                txtSupportEmail: {
                    required: true,
                    email: true
                },
                txtBillingEmail: {
                    required: true,
                    email: true
                },
                txtBlogUrl:
                {
                    url: true
                },
                txtFacebook:
                {
                    url: true
                },
                txtTwitter:
                {
                    url: true
                },
                txtVATNumber:
                {
                    number: true,
                    required: "#chkVAT:checked"
                },
                txtPageSize:
                {
                    number: true
                }
            },
            messages: {
                ddCurrancy: currency,
                drpTimezone: timez,
                drpVATCountry: Vatcountry,
                txtVATNumber: VatNo,
                txtAdminEmail: AdminEmail,
                txtSupportEmail: SuppEmail,
                txtBillingEmail: BillEmail,
                txtBlogUrl: BlogUrl,
                txtFacebook: fb,
                txtTwitter: twitter,
                txtVATNumber: { number: validvat, require: VatPer },
                txtPageSize: page,
                txtCommonName: { required: Commonnm }
            }


        });



        $('#frmSetting').fileUploadUI({

            url: 'SiteSettings/SaveLogo',
            method: 'POST',
            uploadTable: function () { if (this.fileInput[0].id != 'fupBanner') { return $('#files'); } else { return $('#files1'); } },
            downloadTable: function () { if (this.fileInput[0].id != 'fupBanner') { return $('#files'); } else { return $('#files1'); } },
            beforeSend: function (event, files, index, xhr, handler, callBack) {

                var regexp = /\.(png)|(jpeg)|(bmp)|(jpg)|(gif)$/i;
                if (!regexp.test(files[index].name)) {
                    handler.uploadRow.find('.file_upload_progress').html('<span class="redfont">ONLY IMAGES ALLOWED!</span>');
                    setTimeout(function () {
                        handler.removeNode(handler.uploadRow);
                    }, 10000);
                    return;
                }
                if (files[index].size > 1000000) {
                    handler.uploadRow.find('.file_upload_progress').html('<span class="redfont">FILE TOO BIG!</span>');
                    setTimeout(function () {
                        handler.removeNode(handler.uploadRow);
                    }, 10000);
                    return;
                }
                callBack();
            },
            buildUploadRow: function (files, index) {
                return $('<tr><td>' + files[index].name + '<\/td>' +
                            '<td class="file_upload_progress"><div><\/div><\/td>' +
                            '<td class="file_upload_cancel">' +
                            '<button class="ui-state-default ui-corner-all" title="Cancel">' +
                            '<span class="ui-icon ui-icon-cancel">Cancel<\/span>' +
                            '<\/button><\/td><\/tr>');
            },
            buildDownloadRow: function (file) {

                if (file.NeedToCrop) {
                    document.getElementById('dvCropPopupLoader').style.display = 'block';
                    if (file.Type == 'logo')
                        callpcropimagesitesettingsadmin('logo');
                    else
                        callpcropimagesitesettingsadmin('logo');
                }
                else {
                    window.location.href = window.location.href;
                }
                
            },
            fileInputFilter: function (index) {

                if (this.id == 'fupCartButton') {
                    return;
                }
                else {
                    return true;
                }
            }
        });

        //Defalut language is English if none of any language is selected by client:
        setDefaultlanguage();
    });


    function RefreshBrands() {
        $.post("/admin/sitesettings/refreshbrand",
                       function (data) {
                           if (data.toString().toLowerCase().indexOf('error:') == -1) {
                               $('#txtAllowedBrand').val('0,99,' + data);
                               alert(Brand);
                           }
                           else
                               alert(data);
                       });
    }

    function ShowHideFun(status) {
        if (status) {
            document.getElementById('trcountry').style.display = 'table-row';
            document.getElementById('trnumber').style.display = 'table-row';
        }
        else {
            document.getElementById('trcountry').style.display = 'none';
            document.getElementById('trnumber').style.display = 'none';
        }
    }

    function DeleteLogo() {
        $.post("/admin/sitesettings/deletelogo", function (data) {
            if (data.toString() == 'true') {
                alert('logo ' + deleSucss + '!');
                window.location.reload();
            }
            else
                alert(Unabletodel + 'logo !');
        });
    }
    function DeleteCartLogo() {
        $.post("/admin/sitesettings/deletecartlogo", function (data) {
            if (data.toString() == 'true') {
                alert('Cart logo ' + deleSucss + '!');
                window.location.reload();
            }
            else
                alert(Unabletodel + 'logo !');
        });
    }


</script>
